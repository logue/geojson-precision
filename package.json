{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "geojson-precision-ts",
  "version": "1.1.4",
  "license": "MIT",
  "description": "Remove meaningless precision from GeoJSON.",
  "keywords": [
    "geojson",
    "precision",
    "geojson precision"
  ],
  "author": {
    "name": "John J Czaplewski",
    "email": "jczaplew@gmail.com"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    },
    {
      "name": "Frank Rowe",
      "email": "fsrowe@gmail.com",
      "url": "http://frankrowe.org"
    },
    {
      "name": "Andrew Harvey",
      "email": "andrew@alantgeo.com.au",
      "url": "https://www.alantgeo.com.au/"
    },
    {
      "name": "Matthew Jones",
      "email": "matthew@vitaler.com"
    }
  ],
  "homepage": "https://github.com/logue/geojson-precision",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/geojson-precision.git"
  },
  "bugs": {
    "url": "https://github.com/logue/geojson-precision/issues"
  },
  "files": [
    "dist",
    "bin"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "bin": {
    "geojson-precision": "./bin/geojson-precision"
  },
  "engines": {
    "node": ">=18.12.1",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.3.1",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf node_modules/.vite",
    "type-check": "tsc -p tsconfig.vitest.json --noEmit --composite false",
    "build": "run-p type-check build-only build-declaration",
    "build:clean": "rimraf dist",
    "build-only": "vite build",
    "build-declaration": "tsc -p tsconfig.app.json --composite false --declaration --declarationMap --declarationDir dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "test:unit": "vitest --root src/",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^10.0.0"
  },
  "devDependencies": {
    "@mapbox/geojsonhint": "^3.1.0",
    "@types/geojson": "^7946.0.10",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.6.8",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-checker": "^0.5.4",
    "vitest": "^0.28.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3"
  }
}
