{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "geojson-precision-ts",
  "version": "1.1.0",
  "description": "Remove meaningless precision from GeoJSON.",
  "files": [
    "dist",
    "bin"
  ],
  "main": "./dist/geojson-precision.umd.js",
  "module": "./dist/geojson-precision.es.js",
  "exports": {
    ".": {
      "import": "./dist/geojson-precision.es.js",
      "require": "./dist/geojson-precision.umd.js"
    }
  },
  "types": "./dist/geojson-precision.d.ts",
  "bin": {
    "geojson-precision": "./bin/geojson-precision"
  },
  "keywords": [
    "geojson",
    "precision",
    "geojson precision"
  ],
  "author": {
    "name": "John J Czaplewski",
    "email": "jczaplew@gmail.com"
  },
  "contributors": [
    {
      "name": "Logue",
      "email": "logue@hotmail.co.jp",
      "url": "https://logue.dev"
    },
    {
      "name": "Frank Rowe",
      "email": "fsrowe@gmail.com",
      "url": "http://frankrowe.org"
    },
    {
      "name": "Andrew Harvey",
      "email": "andrew@alantgeo.com.au",
      "url": "https://www.alantgeo.com.au/"
    },
    {
      "name": "Matthew Jones",
      "email": "matthew@vitaler.com"
    }
  ],
  "homepage": "https://github.com/logue/geojson-precision",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/geojson-precision.git"
  },
  "bugs": {
    "url": "https://github.com/logue/geojson-precision/issues"
  },
  "engines": {
    "node": ">=16.15.0",
    "yarn": ">=1.22.4"
  },
  "packageManager": "yarn@3.2.1",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf node_modules/.vite",
    "build": "vite build && tsc",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "test": "vite --config vite.config.test.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^9.3.0"
  },
  "devDependencies": {
    "@ricerobotics/geojsonhint": "^3.0.1",
    "@types/geojson": "^7946.0.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "puppeteer": "^14.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-checker": "^0.4.6",
    "vite-plugin-test": "^0.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  }
}
